{
  "name": "maleneaf_site",
  "version": "1.0.0",
  "description": "A project/blogging site",
  "type": "module",
  "scripts": {
    "gen:styles": "node scripts/genStyles.js",
    "build:ssr": "tsc -p tsconfig.ssr.json",
    "build:ssr:watch": "tsc -p tsconfig.ssr.json --watch",
    "dev": "concurrently -k \"node scripts/genStyles.js --watch\" \"npm run build:ssr:watch\" \"vite build --watch\" \"wait-on ssr && NODE_OPTIONS=--experimental-vm-modules eleventy --serve\"",
    "build": "npm run gen:styles && npm run typecheck && npm run build:ssr && vite build && NODE_OPTIONS=--experimental-vm-modules eleventy",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AAFredsted/maleneaf.git"
  },
  "keywords": [
    "ssg",
    "lit",
    "11ty",
    "tsx",
    "ts",
    "vite"
  ],
  "author": "Malene Andr√§-Fredsted",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AAFredsted/maleneaf/issues"
  },
  "homepage": "https://github.com/AAFredsted/maleneaf#readme",
  "dependencies": {
    "@webcomponents/template-shadowroot": "^0.2.1",
    "fs": "^0.0.1-security",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-vite": "^7.1.0-alpha.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@lit-labs/eleventy-plugin-lit": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-lit": "^2.1.1",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "wait-on": "^9.0.3"
  }
}
