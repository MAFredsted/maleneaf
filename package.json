{
  "name": "maleneaf",
  "version": "1.0.0",
  "type": "module",
  "description": "A portfolio and blogging website",
  "scripts": {
    "dev": "concurrently \"npm run dev:11ty\" \"npm run dev:vite\"",
    "dev:11ty": "eleventy --serve --port=8000",
    "dev:vite": "vite --port=3000",
    "build": "npm run lint && npm run build:11ty && npm run build:vite",
    "build:11ty": "eleventy",
    "build:vite": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.js,.tsx",
    "lint:fix": "eslint src --ext .ts,.js,.tsx--fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AAFredsted/maleneaf.git"
  },
  "keywords": [
    "blog",
    "learning",
    "portfolio",
    "articles"
  ],
  "author": "Malene Andr√§-Fredsted",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AAFredsted/maleneaf/issues"
  },
  "homepage": "https://github.com/AAFredsted/maleneaf#readme",
  "dependencies": {
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@types/node": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "11ty.ts": "^0.0.6",
    "concurrently": "^9.2.1",
    "eslint-plugin-lit": "^1.10.1",
    "jiti": "^2.6.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.11"
  }
}
